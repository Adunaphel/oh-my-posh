{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "Base03": "#002b36",
    "Base02": "#073642",
    "Base01": "#586e75",
    "Base00": "#657b83",
    "Base0": "#839496",
    "Base1": "#93a1a1",
    "Base2": "#eee8d5",
    "Base3": "#fdf6e3",
    "Yellow": "#b58900",
    "Orange": "#cb4b16",
    "Red": "#dc322f",
    "Magenta": "#d33682",
    "Violet": "#6c71c4",
    "Blue": "#268bd2",
    "Cyan": "#2aa198",
    "Green": "#859900"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:Violet",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:Violet",
          "style": "plain",
          "template": "{{ if .SSHSession }}\uf817 {{ end }}{{ if .Root }}<p:Red>{{ end }}{{ .UserName }}{{ if .Root }}</>{{ end }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "properties": {
            "folder_icon": "\uf6d7",
            "folder_separator_icon": "\uf460",
            "style": "full",
            "home_icon": "\uf7db"
          },
          "foreground": "p:Magenta",
          "style": "plain",
          "template": "\uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "azure_devops_icon": "\uebe8 "
          },
          "foreground": "p:Yellow",
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "p:Green",
          "templates_logic": "first_match",
          "templates": [
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 25 }}\uf585 {{ end }}{{ end }}", // 20%
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 35 }}\uf586 {{ end }}{{ end }}", // 30%
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 45 }}\uf587 {{ end }}{{ end }}", // 40%
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 65 }}\uf588 {{ end }}{{ end }}", // 60%
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 85 }}\uf589 {{ end }}{{ end }}", // 80%
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 95 }}\uf58a {{ end }}{{ end }}", // 90%
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 101 }}\uf584 {{ end }}{{ end }}", // 100%
            "{{ if lt .Percentage 5 }}<f><p:Red>\uf582</><f> {{ end }}", // empty
            "{{ if lt .Percentage 15 }}<p:Red>\uf579</> {{ end }}", // 10%
            "{{ if lt .Percentage 25 }}<p:Orange>\uf57a</> {{ end }}", // 20%
            "{{ if lt .Percentage 35 }}<p:Orange>\uf57b</> {{ end }}", // 30%
            "{{ if lt .Percentage 45 }}<p:Yellow>\uf57c</> {{ end }}", // 40%
            "{{ if lt .Percentage 55 }}<p:Yellow>\uf57d</> {{ end }}", // 50%
            "{{ if lt .Percentage 65 }}<p:Yellow>\uf57e</> {{ end }}", // 60%
            "{{ if lt .Percentage 75 }}\uf57f {{ end }}", // 70%
            "{{ if lt .Percentage 85 }}\uf580 {{ end }}", // 80%
            "{{ if lt .Percentage 95 }}\uf581 {{ end }}", // 90%
            "{{ if lt .Percentage 101 }}\uf578 {{ end }}" // 100%
          ]
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "p:Green",
          "template": "{{ .Percentage }}\uF295 ",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": "\ue234 ",
            "charged_icon": "\uf578 "
          }
        },
        {
          "template": "{{ if eq .Name \"pwsh\" }}\uebc7{{ else }}\uf489{{ end }} {{ .Name }} {{ if eq .Name \"pwsh\" }}{{ .Version }} {{ end }}",
          "foreground": "p:Blue",
          "style": "plain",
          "type": "shell"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:Cyan",
          "style": "plain",
          "properties": {
            "source": "cli"
          },
          "template": " \uebd8 cli {{ .Name }}",
          "type": "az"
        },
        {
          "foreground": "p:Cyan",
          "style": "plain",
          "properties": {
            "source": "pwsh"
          },
          "template": " \uebd8 pwsh {{ .Name }}",
          "type": "az"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "iterm",
          "interactive": true,
          "style": "plain",
          "foreground": "p:Base2",
          "template": "{{ .PromptMark }}"
        },
        {
          "foreground": "p:Base1",
          "style": "plain",
          "template": " \uf1d0 >",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "foreground": "p:Base1",
    "template": "   > "
  },
  "transient_prompt": {
    "foreground": "p:Base1",
    "template": " {{ if eq .Shell \"pwsh\" }}\uebc7{{ else }}\uf489{{ end }} > "
  },
  "console_title_template": "{{ .Shell }} {{ .UserName }}@{{ .HostName }}:{{ .Folder }}",
  "final_space": true,
  "version": 2
}
