{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#6c71c4",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "#6c71c4",
          "style": "plain",
          "template": "{{ if .SSHSession }}\uf817 {{ end }}{{ if .Root }}<#d33682>{{ end }}{{ .UserName }}{{ if .Root }}</>{{ end }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "properties": {
            "folder_icon": "\uf6d7",
            "folder_separator_icon": "\uf460",
            "style": "full",
            "home_icon": "\uf7db"
          },
          "foreground": "#d33682",
          "style": "plain",
          "template": "\uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "foreground": "#b58900",
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#859900",
          "templates_logic": "first_match",
          "templates": [
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 15 }}\uf585 {{ end }}{{ end }}",
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 25 }}\uf586 {{ end }}{{ end }}",
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 35 }}\uf587 {{ end }}{{ end }}",
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 55 }}\uf588 {{ end }}{{ end }}",
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 75 }}\uf589 {{ end }}{{ end }}",
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 85 }}\uf58a {{ end }}{{ end }}",
            "{{ if eq \"Charging\" .State.String }}{{ if lt .Percentage 101 }}\uf584 {{ end }}{{ end }}",
            "{{ if lt .Percentage 15 }}\uf579 {{ end }}",
            "{{ if lt .Percentage 25 }}\uf57a {{ end }}",
            "{{ if lt .Percentage 35 }}\uf57b {{ end }}",
            "{{ if lt .Percentage 45 }}\uf57c {{ end }}",
            "{{ if lt .Percentage 55 }}\uf57d {{ end }}",
            "{{ if lt .Percentage 65 }}\uf57e {{ end }}",
            "{{ if lt .Percentage 75 }}\uf57f {{ end }}",
            "{{ if lt .Percentage 75 }}\uf580 {{ end }}",
            "{{ if lt .Percentage 85 }}\uf581 {{ end }}",
            "{{ if lt .Percentage 101 }}\uf578 {{ end }}"
          ]
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#859900",
          "template": "{{ .Percentage }}\uF295 ",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": "\ue234 ",
            "charged_icon": "\uf578 "
          }
        },
        {
          "template": "{{ if eq .Name \"pwsh\" }}\uebc7{{ else }}\uf489{{ end }} {{ .Name }} {{ if eq .Name \"pwsh\" }}{{ .Version }} {{ end }}",
          "foreground": "#268bd2",
          "style": "plain",
          "type": "shell"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#2aa198",
          "style": "plain",
          "type": "az",
          "template": " \ufd03 "
        },
        {
          "foreground": "#2aa198",
          "style": "plain",
          "properties": {
            "source": "cli"
          },
          "template": "cli {{ .Name }} ",
          "type": "az"
        },
        {
          "foreground": "#2aa198",
          "style": "plain",
          "properties": {
            "source": "pwsh"
          },
          "template": "pwsh {{ .Name }} ",
          "type": "az"
        },
        {
          "foreground": "#2aa198",
          "style": "plain",
          "type": "az",
          "template": "\ufd03"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cb4b16",
          "style": "plain",
          "template": " \uf1d0 >",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#cb4b16",
    "template": "   > "
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#cb4b16",
    "template": " {{ if eq .Shell \"pwsh\" }}\uebc7{{ else }}\uf489{{ end }} > "
  },
  "console_title_template": "{{ .Shell }} {{ .UserName }}@{{ .HostName }}:{{ .Folder }}",
  "final_space": true,
  "version": 2
}
