{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#6c71c4",
          "foreground": "#eee8d5",
          "leading_diamond": "\ue0c2",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#6c71c4",
          "foreground": "#eee8d5",
          "style": "diamond",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "powerline_symbol": "\ue0cc",
          "type": "session"
        },
        {
          "background": "#d33682",
          "foreground": "#eee8d5",
          "powerline_symbol": "\ue0cc",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#b58900",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#073642",
          "powerline_symbol": "\ue0cc",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },/*
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\ue0cc",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}\uF295 ",
          "properties": {
            "discharging_icon": "\ue231 ",
            "charging_icon": "\ue234 ",
            "charged_icon": "\ue22f "
          }
        },*/
        {
          "background": "#268bd2",
          "foreground": "#eee8d5",
          "powerline_symbol": "\ue0cc",
          "style": "diamond",
          "template": " {{ if eq .Name \"pwsh\" }}\uebc7{{ else }}\uf489{{ end }} {{ .Name }} {{ if eq .Name \"pwsh\" }}{{ .Version }} {{ end }}",
          "trailing_diamond": "\ue0c0",
          "type": "shell"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "style": "diamond",
          "leading_diamond": "\ue0c2",
          "type": "az",
          "template": "\ufd03"
        },
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "style": "powerline",
          "leading_diamond": "\ue0c2",
          "trailing_diamond": "\ue0c0",
          "powerline_symbol": "\ue0cc",
          "properties": {
            "source": "cli"
          },
          "template": "cli {{ .Name }}",
          "type": "az"
        },
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "style": "powerline",
          "leading_diamond": "\ue0c2",
          "trailing_diamond": "\ue0c0",
          "powerline_symbol": "\ue0cc",
          "properties": {
            "source": "pwsh"
          },
          "template": "pwsh {{ .Name }} ",
          "type": "az"
        },
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "style": "diamond",
          "trailing_diamond": "\ue0c0",
          "type": "az",
          "template": "\ufd03"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cb4b16",
          "style": "plain",
          "template": "\uf1d0 >",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
      "background": "transparent",
      "foreground": "#cb4b16",
      "template": "  > "
  },
  "transient_prompt": {
      "background": "transparent",
      "foreground": "#cb4b16",
      "template": "{{ .Shell }}> "
  },
  "console_title_template": "{{ .Shell }} {{ .UserName }}@{{ .HostName }}:{{ .Folder }}",
  "final_space": true,
  "version": 2
}
